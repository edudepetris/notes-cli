{
  "name": "devnotes-cli",
  "version": "0.0.8",
  "author": "Edu Depetris @edudepetris",
  "bin": {
    "devnotes": "./bin/run"
  },
  "bugs": "https://github.com/edudepetris/notes-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-autocomplete": "^0.3.0",
    "@oclif/plugin-help": "^3",
    "@oclif/plugin-not-found": "^1.2.4",
    "axios": "^0.21.1",
    "fs-extra": "^10.0.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.11",
    "@types/mocha": "^8",
    "@types/node": "^15.0.3",
    "aws-sdk": "^2.1083.0",
    "chai": "^4",
    "eslint": "^7.26.0",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.2",
    "globby": "^10",
    "mocha": "^5",
    "nock": "^13.0.11",
    "nyc": "^14",
    "sinon": "^10.0.0",
    "ts-node": "^9",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://edudepetris.github.io/devnotes.github.io/",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "devnotes",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-autocomplete"
    ],
    "update": {
      "s3": {
        "bucket": "devnotes-cli"
      }
    }
  },
  "repository": "edudepetris/notes-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
